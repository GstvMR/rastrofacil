{
  "name": "rastrofacil",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc -p .",
    "start": "node dist/app.js",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "format": "prettier --write .",
    "worker:pending": "ts-node-dev --respawn --transpile-only src/workers/pendingLeadsWorker.ts",
    "worker:route": "ts-node-dev --respawn --transpile-only src/workers/routeSimulatorWorker.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "argon2": "^0.43.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "resend": "^4.5.1",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
